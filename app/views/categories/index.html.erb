<%= 'Categories:' %>
<table class="table">
  <tr>
    <td><h4><%= t('cat_name') %></h4></td>
    <td><h4><%= t('cat_icon') %></h4></td>
    <td><h4><%= t('cat_created') %></h4></td>
    <td><h4><%= t('cat_updated') %></h4></td>
  </tr>
  <% @category.each do |c| %>
    <tr>
      <td><%= c.name %></td>
      <td><%= c.icon %></td>
      <td><%= c.created_at%></td>
      <td><%= c.updated_at%></td>
    </tr>
  <%end%>

</table>

<%= link_to t('people'), me_people_path %>
<%= link_to t('Categories'), categories_path %>
