<div id="profile-card" class="card profile">
  <div id="profile-picture">
    <div class="img-container">
      <%= image_tag "test/profile-picture.jpg" %>
    </div>
  </div>
  <h1><%= @name %></h1>
  <h2><%= @trole %></h2>
  <ul id="skills">
    <% @skills.each do |s| %>
        <li><%= image_tag s.icon %></li>
    <% end %>
  </ul>
</div>